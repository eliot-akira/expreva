{
  "name": "expreva",
  "version": "2.0.0",
  "description": "Expreva - A language based on arithmetic and algebra expressions",
  "scripts": {
    "dev": "cd example && tgb dev",
    "build": "tgb build",
    "tdd": "concurrently --prefix none --handle-input 'sleep 2 && node src/tests' 'tgb dev'",
    "test": "if [ ! -d \"build\" ]; then yarn build; fi; node src/tests",
    "type": "tsc --declaration --emitDeclarationOnly",
    "deploy": "./deploy"
  },
  "bin": {
    "expr": "run"
  },
  "main": "index.js",
  "browser": "web/expreva.js",
  "files": [
    "**/*.js",
    "readme.md"
  ],
  "dependencies": {},
  "devDependencies": {
    "@tangible/builder": "^1.3.2",
    "concurrently": "^5.0.0",
    "testra": "^1.0.2"
  },
  "keywords": [
    "expression",
    "parse",
    "evaluate",
    "language"
  ],
  "homepage": "https://expreva.com/",
  "repository": "git+https://github.com/eliot-akira/expreva.git",
  "license": "MIT"
}

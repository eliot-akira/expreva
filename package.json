{
  "name": "expreva",
  "version": "2.0.0",
  "description": "Expreva - A language based on arithmetic and algebra expressions",
  "scripts": {
    "dev": "concurrently --raw \"rollup -c rollup.config.js -w\" \"node dev-server\"",
    "build": "rollup -c rollup.min.config.js",
    "test": "if [ ! -d \"build\" ]; then yarn build; fi; tester src/tests",
    "deploy": "./deploy"
  },
  "main": "index.js",
  "browser": "build/expreva.js",
  "files": [
    "**/*.js",
    "readme.md"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime-corejs3": "^7.6.2",
    "@mna/tester": "2.x",
    "concurrently": "^5.0.0",
    "core-js": "^3.2.1",
    "onchange": "^6.1.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.3"
  },
  "keywords": [
    "expression",
    "parse",
    "evaluate",
    "language"
  ],
  "homepage": "https://expreva.com/",
